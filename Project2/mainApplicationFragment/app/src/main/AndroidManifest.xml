<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gkn.squad.mainapplicationfragment">

    <permission
        android:name="edu.uic.cs478.sp18.project3"
        android:label="deadlyActivity"
        android:permissionGroup="android.permission-group.GKN"
        android:protectionLevel="dangerous" />

    <uses-permission android:name="edu.uic.cs478.sp18.project3"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
